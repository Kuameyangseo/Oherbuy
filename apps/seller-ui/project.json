{
  "name": "seller-ui",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/seller-ui",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project seller-ui --web",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/seller-ui",
        "root": "apps/seller-ui"
      },
      "configurations": {
        "production": {}
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx next dev",
        "cwd": "apps/seller-ui",
        "parallel": false
      }
    }
  },
  "scripts": {
    "seed:sellers": "ts-node src/scripts/seed-seller.ts"
  }
}
